<?xml version="1.0" encoding="utf-16"?>
<AppSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <minRamdomConfig>100</minRamdomConfig>
  <maxRamdomConfig>400</maxRamdomConfig>
  <DefaultTimer>0</DefaultTimer>
  <BetweenKeys>true</BetweenKeys>
  <ForeachKeys>false</ForeachKeys>
  <HotKeyStartRecord>Control + F1</HotKeyStartRecord>
  <HotKeyStopRecord>Control + F2</HotKeyStopRecord>
  <HotKeyPlay>Control + J</HotKeyPlay>
  <StopMacroControl>
    <HotKeyStopModifiers>
      <Keys>LControlKey</Keys>
    </HotKeyStopModifiers>
    <HotKeyStopKeys>
      <Keys>L</Keys>
    </HotKeyStopKeys>
  </StopMacroControl>
</AppSettings>